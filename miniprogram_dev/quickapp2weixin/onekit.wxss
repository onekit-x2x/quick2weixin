page {
    width: 100%;
    height: 100%;
    background-color: #fff;
    font-size: 4vw;
    font-family: sans-serif,PingFang SC;
}
.onekit-div,.onekit-list,.onekit-list-item,.onekit-popup,.onekit-refresh,.onekit-richtext,.onekit-stack,.onekit-swiper,.onekit-tabs,.onekit-tab-bar,.onekit-tab-content,.onekit-a,.onekit-image,.onekit-rating,.onekit-span,.onekit-text,.onekit-marquee,.onekit-input,.onekit-lable,.onekit-option,.onekit-picker,.onekit-select,.onekit-textarea,.onekit-video,.onekit-camera,.onekit-canvas,.onekit-web,.onekit-map,.onekit-custommarker {
    margin: 0;
    padding: 0;
    border-width: 0;
    text-size-adjust: none;
}
.onekit-tab-bar>.onekit-active{
    color:#0faeff!important;
}
page>.onekit-div{ /* page>div */
    margin: 0 auto;
    pointer-events: auto;
    height: 100%;
}
/************** div *****************/
.onekit-div {
    box-sizing: border-box;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    flex: 0 1 auto;
    z-index: 0;
    max-width: 100%;
}
.onekit-div {
    pointer-events: none;
    overflow: hidden;
}
page>.onekit-div {
    
}
/*list*/
.onekit-list {
    box-sizing: border-box;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    flex: 0 1 auto;
    z-index: 0;
    max-width: 100%;
}
.onekit-list{
    display: block;
    overflow: hidden;
    height: 100%;
    width: -webkit-fill-available;
    flex: 0 999999 auto;
    pointer-events: auto;
}
.onekit-list{
    
}
/*.onekit-list-item**/
.onekit-list-item{
    box-sizing: border-box;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    z-index: 0;
    overflow: hidden;
}
.onekit-list-item {
    height: fit-content;
    max-width: 100vw;
}
.onekit-list-item{
    
}
/******/
.onekit-popup{
    box-sizing: border-box;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    flex: 0 1 auto;
    z-index: 0;
    max-width: 100%;
}
.onekit-popup {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    display: none;
    z-index: 999;
}
.onekit-popup {
    
}
/******/
.onekit-refresh {
    box-sizing: border-box;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    flex: 0 1 auto;
    z-index: 0;
    width: 100%;
    height: 100%;
    max-width: 100%;
    pointer-events: auto;
}
.onekit-refresh {
    
}
/* .onekit-richtext */
.onekit-richtext{
    pointer-events: auto;
    width: -webkit-fill-available;
    text-size-adjust: 200%;
    padding-bottom: 20rpx;
}
.onekit-richtext{
    
}
/* .onekit-stack */
.onekit-stack {
    width: 0rpx;
    
}
/*.onekit-swiper  */
.onekit-swiper {
    position: relative;
    width: -webkit-fill-available;
}
.onekit-swiper {
    position: relative;
    overflow: hidden;
    
}
/* .onekit-tabs */
.onekit-tabs {
    box-sizing: border-box;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    flex: 0 1 auto;
    z-index: 0;
    max-width: 100%;
}
.onekit-tabs {
    position: relative;
    flex-direction: column;
    width: -webkit-fill-available;
    max-width: 100vw;
    overflow-x: hidden;
    flex-grow: 1;
    flex-shrink: 999999;
    pointer-events: auto;
    max-height: 100%;
}
/* .onekit-tab-bar */
.onekit-tab-bar {
   display: flex;
    width: 100%;
    align-items: center;
    height: 120px;
    flex-shrink: 0;
    border-top: 1px solid rgb(187, 187, 187);
    border-right-color: rgb(187, 187, 187);
    border-bottom-color: rgb(187, 187, 187);
    border-left-color: rgb(187, 187, 187);
    background-color: rgb(255, 255, 255);
    bottom: 0px;
}
/* .onekit-tab-content */
.onekit-tab-content {
    height: 100%;
    width: 100%;
    overflow-x: hidden;
    overflow-y: scroll;
    display: block;
}
.onekit-tab-content {
    height: calc(100% - 13.2px);
}
/*  .onekit-a*/
.onekit-a {
    pointer-events: auto;
    overflow: hidden;
}
.onekit-a {
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-box-pack: center;
    word-break: break-word;
    box-sizing: border-box;
    line-height: normal;
    z-index: 0;
    text-align: left;
    width: fit-content;
    white-space: pre-line;
}
.onekit-a {
    color: grey;
    text-decoration: none;
    
}
/* .onekit-image */
.onekit-image {
    display: flex;
    flex: 0 1 auto;
    box-sizing: border-box;
    position: relative;
    border: 0 solid #000;
    margin: 0;
    padding: 0;
    min-width: 0;
    white-space: normal;
    overflow: hidden;
}
.onekit-image {
    width: auto;
    height: auto;
    overflow: hidden;
    
}
/*  .onekit-rating*/
.onekit-rating {
    -webkit-flex-grow: 0;
    flex-grow: 0;
    -webkit-flex-basis: auto;
    flex-basis: auto;
    position: relative;
    border: 0 solid #000;
    margin: 0;
    padding: 0;
    min-width: 0;
}
.onekit-rating {
    box-sizing: border-box;
    -webkit-flex-shrink: 0;
    flex-shrink: 0;
    width: auto;
    white-space: normal;
    pointer-events: auto;
}
.onekit-rating {
    display: flex;
    position: relative;
    width: 66.67vw;
    height: 13.33vw;
    justify-content: space-around;
    overflow: hidden;
}
.onekit-rating {
    
}
/*.onekit-span  */
.onekit-span {
    
}
/* .onekit-text */
.onekit-text {
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-box-pack: center;
    word-break: break-word;
    box-sizing: border-box;
    line-height: normal;
    z-index: 0;
    text-align: left;
    width: fit-content;
    white-space: pre-line;
}
.onekit-text {
    
}
/* .onekit-marquee */
.onekit-marquee {
    display: inline-block;
    width: -webkit-fill-available;
}
.onekit-marquee {
    display: inline-block;
    overflow: hidden;
    text-align: initial;
    white-space: nowrap;
}
.onekit-marquee {
    
}
/*.onekit-input  */
.onekit-input {
    writing-mode: horizontal-tb !important;
    text-rendering: auto;
    color: -internal-light-dark-color(black, white);
    letter-spacing: normal;
    word-spacing: normal;
    text-transform: none;
    text-indent: 0rpx;
    text-shadow: none;
    display: inline-block;
    text-align: start;
    -webkit-appearance: textfield;
    background-color: -internal-light-dark-color(rgb(255, 255, 255), rgb(59, 59, 59));
    -webkit-rtl-ordering: logical;
    cursor: text;
    margin: 0em;
    font: 400 13.3333rpx Arial;
    padding: 1rpx 2rpx;
    border-width: 2rpx;
    border-style: inset;
    border-color: -internal-light-dark-color(rgb(118, 118, 118), rgb(195, 195, 195));
    border-image: initial;
}
.onekit-input {
    box-sizing: border-box;
    position: relative;
    -webkit-flex-shrink: 1;
    flex-shrink: 1;
    -webkit-flex-grow: 0;
    flex-grow: 0;
    -webkit-flex-basis: auto;
    flex-basis: auto;
    border: 0 solid #000;
    margin: 0;
    padding: 0;
    min-width: 0;
    /* width: auto; */
    white-space: normal;
    pointer-events: auto;
}
.onekit-input {
    background-color: inherit;
    outline: none;
    font-size: 3.47vw;
    
}
/*.onekit-label  */
.onekit-label {
    cursor: default;
}
.onekit-label {
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-box-pack: center;
    word-break: break-word;
    box-sizing: border-box;
    line-height: normal;
    z-index: 0;
    text-align: left;
    width: fit-content;
    white-space: pre-line;
}
.onekit-label {
    font-size: 4.27vw;
    
}
/*.onekit-option*/
.onekit-option {
    font-weight: normal;
    display: block;
    white-space: pre;
    min-height: 1.2em;
    padding: 0rpx 2rpx 1rpx;
}
.onekit-option {
    pointer-events: auto;
    
}
/* .onekit-picker */
.onekit-picker {
    -webkit-flex-grow: 0;
    flex-grow: 0;
    -webkit-flex-basis: auto;
    flex-basis: auto;
    position: relative;
    border: 0 solid #000;
    margin: 0;
    padding: 0;
    min-width: 0;
}
.onekit-picker {
    box-sizing: border-box;
    -webkit-flex-shrink: 0;
    flex-shrink: 0;
    width: auto;
    white-space: normal;
    pointer-events: auto;
}
.onekit-picker {
    font-size: 4.27vw;
    align-self: center;
    
}
/* .onekit-select */
.onekit-select {
    writing-mode: horizontal-tb !important;
    text-rendering: auto;
    color: -internal-light-dark-color(black, white);
    letter-spacing: normal;
    word-spacing: normal;
    text-transform: none;
    text-indent: 0rpx;
    text-shadow: none;
    display: inline-block;
    text-align: start;
    -webkit-appearance: menulist;
    box-sizing: border-box;
    align-items: center;
    white-space: pre;
    -webkit-rtl-ordering: logical;
    background-color: -internal-light-dark-color(rgb(255, 255, 255), rgb(59, 59, 59));
    cursor: default;
    margin: 0em;
    font: 400 13.3333rpx Arial;
    border-radius: 0rpx;
    border-width: 1rpx;
    border-style: solid;
    border-color: -internal-light-dark-color(rgb(118, 118, 118), rgb(195, 195, 195));
    border-image: initial;
}
.onekit-select:not(:-internal-list-box) {
    overflow: visible !important;
}
.onekit-select {
    
}
/* .onekit-textarea */
.onekit-textarea {
    writing-mode: horizontal-tb !important;
    text-rendering: auto;
    color: -internal-light-dark-color(black, white);
    letter-spacing: normal;
    word-spacing: normal;
    text-transform: none;
    text-indent: 0rpx;
    text-shadow: none;
    display: inline-block;
    text-align: start;
    -webkit-appearance: textarea;
    background-color: -internal-light-dark-color(rgb(255, 255, 255), rgb(59, 59, 59));
    -webkit-rtl-ordering: logical;
    flex-direction: column;
    resize: auto;
    cursor: text;
    white-space: pre-wrap;
    overflow-wrap: break-word;
    margin: 0em;
    font: 400 13.3333rpx Arial;
    border-width: 1rpx;
    border-style: solid;
    border-color: -internal-light-dark-color(rgb(118, 118, 118), rgb(195, 195, 195));
    border-image: initial;
    padding: 2rpx;
}
.onekit-textarea {
    -webkit-flex-grow: 0;
    flex-grow: 0;
    -webkit-flex-basis: auto;
    flex-basis: auto;
    position: relative;
    border: 0 solid #000;
    margin: 0;
    padding: 0;
    min-width: 0;
}
.onekit-textarea {
    box-sizing: border-box;
    -webkit-flex-shrink: 0;
    flex-shrink: 0;
   /* width: auto;*/
    white-space: normal;
    pointer-events: auto;
}
.onekit-textarea {
    font-size: 4.27vw;
    background-color: inherit;
  
}
/* .onekit-video */
.onekit-video {
    object-fit: contain;
}
.onekit-video {
    -webkit-flex-grow: 0;
    flex-grow: 0;
    -webkit-flex-basis: auto;
    flex-basis: auto;
    position: relative;
    border: 0 solid #000;
    margin: 0;
    padding: 0;
    min-width: 0;
}
.onekit-video {
    box-sizing: border-box;
    -webkit-flex-shrink: 0;
    flex-shrink: 0;
    width: auto;
    white-space: normal;
    pointer-events: auto;
}
.onekit-video {
    background-color: #000;
    height: 100%;
}
.onekit-video {
    
}
/*camera  */
.onekit-camera {
    box-sizing: border-box;
